      <tr>
        <td><%= employee.name %></td>
        <td><%= employee.age %></td>
        <td><%= employee.phone_num %></td>
        <td><%= employee.College %></td>
      <td><iframe src="abc.pdf" width="140" height="200" style="border: none;"> </iframe>      </td>  
        <td><%= link_to 'Show', employee %></td>
